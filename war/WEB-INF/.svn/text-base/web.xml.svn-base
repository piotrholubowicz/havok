<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>GWT-Maven-Archetype</display-name>

	<welcome-file-list>
		<welcome-file>Havok.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ActionServiceImpl</servlet-name>
		<servlet-class>pl.poznan.ilim.havok.server.ActionServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ActionServiceImpl</servlet-name>
		<url-pattern>/Havok/actionservice</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ItemServiceImpl</servlet-name>
		<servlet-class>pl.poznan.ilim.havok.server.ItemServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ItemServiceImpl</servlet-name>
		<url-pattern>/Havok/itemservice</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ObservationServiceImpl</servlet-name>
		<servlet-class>pl.poznan.ilim.havok.server.ObservationServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ObservationServiceImpl</servlet-name>
		<url-pattern>/Havok/observationservice</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>StatusServiceImpl</servlet-name>
		<servlet-class>pl.poznan.ilim.havok.server.StatusServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StatusServiceImpl</servlet-name>
		<url-pattern>/Havok/statusservice</url-pattern>
	</servlet-mapping>

</web-app>
